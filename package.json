{
  "name": "pipem",
  "version": "0.0.3",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.map",
    "dist/*.ts"
  ],
  "author": "Miroslav Vršecký <yamiteru@icloud.com>",
  "description": "Async, abortable and composable pipe with a bunch of helpful utils.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamiteru/pipem.git",
    "directory": "src"
  },
  "keywords": [
    "pipe",
    "pipe-operator",
    "functional-programming",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamiteru/pipem/issues"
  },
  "homepage": "https://github.com/yamiteru/pipem",
  "scripts": {
    "fix:format": "prettier --write \"**/*.ts\"",
    "fix:lint": "eslint --fix --ext .ts .",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:lint": "eslint --ext .ts .",
    "check": "pnpm check:lint && pnpm check:format",
    "test:coverage": "jest --collectCoverage",
    "test": "jest",
    "build": "pnpm clean && ts-node scripts/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "start": "ts-node src/test.ts",
    "release": "release-it",
    "benchmark": "ts-node benchmarks/index.ts"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "3.1.0",
    "@swc/cli": "0.1.59",
    "@swc/core": "1.3.28",
    "@swc/helpers": "0.4.14",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "auto-changelog": "2.4.0",
    "esbuild": "0.17.4",
    "esbuild-plugin-swc": "1.0.1",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "husky": "8.0.3",
    "jest": "29.4.0",
    "npm-dts": "1.3.12",
    "prettier": "2.8.3",
    "release-it": "15.6.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
