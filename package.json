{
  "name": "pipem",
  "version": "0.0.1",
  "license": "MIT",
	"type": "module",
  "scripts": {
    "fix:format": "prettier --write \"**/*.ts\"",
    "fix:lint": "eslint --fix --ext .ts .",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:lint": "eslint --ext .ts .",
    "check": "pnpm check:lint && pnpm check:format",
    "test:coverage": "jest --collectCoverage",
    "test": "jest",
    "build": "ts-node --esm scripts/build.ts",
    "prepublishOnly": "pnpm test && pnpm build",
    "release": "release-it",
    "benchmark": "ts-node --esm benchmarks/index.ts"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "3.1.0",
    "@swc/cli": "0.1.59",
    "@swc/core": "1.3.28",
    "@swc/helpers": "0.4.14",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "auto-changelog": "2.4.0",
    "esbuild": "0.17.4",
    "esbuild-plugin-swc": "1.0.1",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "husky": "8.0.3",
    "jest": "29.4.0",
    "prettier": "2.8.3",
    "release-it": "15.6.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
